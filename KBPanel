import java.util.*;
import javax.swing.*;
import java.util.*;
import java.io.*;
import java.awt.*;
import java.awt.event.*;
import java.awt.image.*;
import javax.imageio.*;

public class KingdomBuilderTest extends JPanel{
	private static BufferedImage board1, board2, board3, board4;
	
	public KingdomBuilderTest() {
		
		try {
			board1 = ImageIO.read(this.getClass().getResource("/Images/Board1Transparent.png"));
			board2 = ImageIO.read(this.getClass().getResource("/Images/Board2Transparent.png"));
			board3 = ImageIO.read(this.getClass().getResource("/Images/Board3Transparent.png"));
			board4 = ImageIO.read(this.getClass().getResource("/Images/Board4Transparent.png"));
			
		}catch(Exception e) { System.out.println("Can't read images"); }
		
		repaint();
		
	}
	
	
	public void paint(Graphics g) {
		g.drawImage(board1, 0, 0, 500, 500, null);

	}

}
